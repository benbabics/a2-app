<?xml version='1.0' encoding='utf-8'?>
<widget android-versionCode="1" id="com.wex.wol.AccountMaintenance" ios-CFBundleVersion="1" version="1.1.2" xmlns="http://www.w3.org/ns/widgets" xmlns:gap="http://phonegap.com/ns/1.0">
    <preference name="loglevel" value="DEBUG" />
    <feature name="ApplicationInfo">
        <param name="android-package" value="com.wex.plugins.applicationInfo.ApplicationInfoPlugin" />
    </feature>
    <feature name="Device">
        <param name="android-package" value="org.apache.cordova.device.Device" />
    </feature>
    <feature name="NetworkStatus">
        <param name="android-package" value="org.apache.cordova.networkinformation.NetworkManager" />
    </feature>
    <feature name="SplashScreen">
        <param name="android-package" value="org.apache.cordova.splashscreen.SplashScreen" />
    </feature>
    <feature name="StatusBar">
        <param name="android-package" onload="true" value="org.apache.cordova.statusbar.StatusBar" />
    </feature>
    <preference name="ErrorUrl" value="error.html" />
    <preference name="LoadUrlTimeoutValue" value="18000" />
    <preference name="KeepRunning" value="false" />
    <preference name="SplashScreen" value="splash" />
    <preference name="SplashScreenDelay" value="20000" />
    <name>WEXonline</name>
    <description>
        Account Maintenance for Classic and WOL NP customers.
    </description>
    <author email="MobileDevelopment@wexinc.com" href="http://www.wexinc.com">
        WEX Mobile Development Team
    </author>
    <content src="index.html" />
    <preference name="Fullscreen" value="false" />
    <preference name="Orientation" value="portrait" />
    <preference name="DisallowOverscroll" value="false" />
    <access origin="*" />
    <access origin="*.wexmobile.com" />
</widget>
